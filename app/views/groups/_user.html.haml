- unless user.name === current_user.name then
  .chat-group-user.clearfix
    %input{name: "group[user_ids][]", type: "hidden", value: user.id}/
    %p.chat-group-user__name
      = user.name
    %a{class: "chat-group-user__btn chat-group-user__btn--remove", user_id: user.id, user_name: user.name}削除
